<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="android.example.tanggalwaktu">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Rate"
            android:label="@string/rate"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".DaftarIsi"
            android:label="@string/daftarisi"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".lokasiZiarah"
            android:label="@string/lokasi"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Tentang"
            android:label="@string/tentang"
            android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".search"
            android:parentActivityName=".MainActivity">
        </activity>
    </application>

</manifest>